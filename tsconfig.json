{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "strict": false,
    "strictNullChecks": false,
    "paths": {
      "@/*": [
        "./*"
      ],
      "crypto": [
        "node_modules/crypto-js"
      ]
    }
  },
  
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    ".env"
  ]
}
