{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "strict": false,
    "strictNullChecks": false,
    "paths": {
      "@/*": [
        "./*",
        
      ],
      "crypto": [
        "node_modules/crypto-js"
      ]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    ".env"
  ]
}